{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build:vercel",
    "outputDirectory": "dist/public",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}